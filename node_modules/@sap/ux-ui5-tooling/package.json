{
  "name": "@sap/ux-ui5-tooling",
  "displayName": "SAP Fiori Tools – UI5 Tooling",
  "version": "1.20.2",
  "description": "SAP Fiori Tools – UI5 Tooling",
  "publisher": "SAPSE",
  "license": "SEE LICENSE IN LICENSE",
  "bin": {
    "fiori": "bin/fiori"
  },
  "private": false,
  "scripts": {
    "clean:dist": "rimraf --glob ./dist ./generators *.tsbuildinfo",
    "clean": "rimraf ./.webpack_cache",
    "build:compile": "tsc --build ./ --pretty --dry",
    "build:bundle": "node esbuild.js",
    "build:copyTemplates1": "copyfiles --error --flat -V views/** ./dist/views",
    "build:copyTemplates2": "copyfiles --error -V src/markdowns/**/*.* ./dist/ -u 1",
    "watch": "yarn clean && node esbuild --watch --minify=false",
    "build": "npm-run-all -l -s clean -p build:compile build:bundle build:copyTemplates1 build:copyTemplates2",
    "build:deps": "yarn lerna run --concurrency 1 --no-bail --stream --scope @sap/ux-app-templates build",
    "bundle": "yarn build:deps && yarn build && npm pack",
    "test": "cross-env SAP_UX_FIORI_TOOLS_DISABLE_TELEMETRY=true FIORI_TOOLS_DISABLE_SECURE_STORE=true jest --maxWorkers=1 --ci --forceExit --detectOpenHandles",
    "test:sequence": "node -r ts-node/register test/sample-project/app/request-sequence.ts",
    "test:cpe-integration": "cross-env SAP_UX_FIORI_TOOLS_DISABLE_TELEMETRY=true playwright test",
    "test:cpe-integration-adp": "cross-env SAP_UX_FIORI_TOOLS_DISABLE_TELEMETRY=true playwright test --project=ADP",
    "test:cpe-integration-cpe": "cross-env SAP_UX_FIORI_TOOLS_DISABLE_TELEMETRY=true playwright test --project=CPE",
    "lint": "eslint . ",
    "lint:summary": "eslint .  -f summary",
    "lint:fix": "eslint --fix",
    "lint:fix:all": "eslint .  --fix",
    "lint:report": "eslint .  -f multiple ",
    "format:fix": "prettier --write --loglevel silent --ignore-path ../../../.prettierignore",
    "format:fix:all": "prettier --write '**/*.{css,scss,html,js,json,ts,tsx,yaml,yml}' '!**/{out,dist,typings,node_modules}/**' '!**/*.{svg,png,xml}' --ignore-path ../../../.prettierignore",
    "tgz:package": "npm pack",
    "madge": "madge --warning --circular --extensions ts ./"
  },
  "dependencies": {
    "@sap-ux/preview-middleware": "0.23.94",
    "@ui5/fs": "4.0.3",
    "@ui5/manifest": "1.81.0",
    "connect-livereload": "0.6.1",
    "express": "4.22.1",
    "livereload": "0.10.3",
    "semver": "7.7.2",
    "update-notifier-cjs": "5.1.7"
  },
  "optionalDependencies": {
    "@zowe/secrets-for-zowe-sdk": "8.27.0"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@sap-ux/axios-extension": "1.25.6",
    "@sap-ux/backend-proxy-middleware": "0.10.29",
    "@sap-ux/btp-utils": "1.1.6",
    "@sap-ux/deploy-tooling": "0.17.32",
    "@sap-ux/feature-toggle": "0.3.5",
    "@sap-ux/guided-answers-helper": "0.4.2",
    "@sap-ux/logger": "0.8.0",
    "@sap-ux/nodejs-utils": "0.2.11",
    "@sap-ux/project-access": "1.34.2",
    "@sap-ux/reload-middleware": "0.3.10",
    "@sap-ux/serve-static-middleware": "0.4.7",
    "@sap-ux/store": "1.5.0",
    "@sap-ux/system-access": "0.6.41",
    "@sap-ux/telemetry": "0.6.56",
    "@sap-ux/ui5-config": "0.29.13",
    "@sap-ux/ui5-info": "0.13.8",
    "@sap-ux/ui5-proxy-middleware": "1.6.12",
    "@sap/ux-app-templates": "1.20.2",
    "@sap/ux-specification": "1.139.2",
    "@types/fs-extra": "11.0.4",
    "@types/marked": "4.0.1",
    "@types/node": "20.12.11",
    "@types/prompts": "2.4.9",
    "@types/supertest": "6.0.3",
    "axios": "1.13.2",
    "chalk": "4.1.2",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "fs-extra": "11.3.2",
    "http-proxy-middleware": "3.0.5",
    "i18next": "25.5.3",
    "marked": "16.4.0",
    "marked-terminal": "7.3.0",
    "nock": "14.0.10",
    "portfinder": "1.0.38",
    "prompts": "2.4.2",
    "serve-static": "2.2.0",
    "supertest": "7.1.4",
    "tree-kill": "1.2.2",
    "typescript": "5.9.2",
    "wait-for-expect": "4.0.0",
    "yaml": "2.8.1",
    "yargs-parser": "22.0.0"
  },
  "ui5": {
    "dependencies": []
  },
  "files": [
    "bin",
    "dist",
    "doc",
    "README.md",
    "CHANGELOG.md",
    "package.json",
    "ui5.yaml",
    "apply-patches.js",
    "patches",
    "!*.tsbuildinfo",
    "!**/*.tsbuildinfo",
    "!*.map",
    "!**/*.map"
  ],
  "jestSonar": {
    "reportPath": "reports/test/unit",
    "reportFile": "test-report.xml"
  },
  "eslint-formatter-multiple": {
    "formatters": [
      {
        "name": "stylish",
        "output": "console"
      },
      {
        "name": "json",
        "output": "file",
        "path": "reports/lint/eslint.json"
      },
      {
        "name": "checkstyle",
        "output": "file",
        "path": "reports/lint/eslint.checkstyle.xml"
      }
    ]
  }
}
