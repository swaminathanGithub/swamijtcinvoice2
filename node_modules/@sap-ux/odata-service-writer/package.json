    "ejs": "3.1.10",
    "fast-xml-parser": "4.4.1",
    "i18next": "25.3.0",
    "mem-fs": "2.1.0",
    "mem-fs-editor": "9.4.0",
    "prettify-xml": "1.2.0",
    "semver": "7.5.4",
    "@sap-ux/mockserver-config-writer": "0.9.40",
    "@sap-ux/project-access": "1.34.2",
    "@sap-ux/ui5-config": "0.29.13"
  },
  "devDependencies": {
    "@sap-ux/vocabularies-types": "0.14.5",
    "@types/ejs": "3.1.2",
    "@types/fs-extra": "9.0.13",
    "@types/mem-fs": "1.1.2",
    "@types/mem-fs-editor": "7.0.1",
    "@types/semver": "7.5.2",
